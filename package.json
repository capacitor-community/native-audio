{
  "name": "@bazumax/native-audio",
  "version": "0.2.1",
  "description": "A native plugin for native audio engine",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "npm-publish": "np"
  },
  "author": "bazuka5801 <bazuka5801@mail.ru>",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "^3.0.0-beta.6"
  },
  "devDependencies": {
    "@capacitor/android": "^3.0.0-beta.6",
    "@capacitor/ios": "^3.0.0-beta.6",
    "husky": "^4.2.5",
    "np": "^6.2.4",
    "prettier": "^2.0.5",
    "prettier-plugin-java": "^0.8.0",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "BazumaxNativeAudio.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bazuka5801/native-audio"
  },
  "bugs": {
    "url": "https://github.com/bazuka5801/native-audio/issues"
